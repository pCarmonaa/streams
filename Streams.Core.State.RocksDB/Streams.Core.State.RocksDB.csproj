<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0-alpha2</Version>
    <PackageId>Com.RFranco.Streams.Core.State.RocksDB</PackageId>
    <Title>Streams.Core.State.RocksDB</Title>
    <Description>Library to manage application state using RocksDB.</Description>
    <Authors>Ruben Martinez;Javier Solana;R.Franco Techonologies</Authors>
    <Copyright>Copyright R.Franco Techonologies 2020</Copyright>
    <PackageTags>Application State RocksDB</PackageTags>
    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/rfrancotechnologies/streams</PackageProjectUrl>
    <IncludeSource>false</IncludeSource>
    <PackageReleaseNotes>
      Allow one to many relationship between states and storage.
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RocksDbSharp" Version="5.17.2" />
    <PackageReference Include="Com.RFranco.Streams" Version="1.0.0-alpha3" />    
  </ItemGroup>    
</Project>
